        <div class="button py-3">
            <div id="button-section" class="offset-1 col-lg-10">
            </div>

        </div>

        <!-- div for getting Admins all blog -->
        <div id="show-admin-all-blogs">

        </div>


        <div id="edit-admin-blog-section">

        </div>

        <!-- section to edit the blog -->

        <div class="container-fluid " id="edit-blog-section">
            <!-- to get space from sides -->
            <div class="container-f col-xl-6 col-lg-6 col-md-11 col-11 mx-auto mt-2
                text-center">

                <h1 class="mb-4 mt-3 new_blog">New Article</h1>

                <form enctype="multipart/form-data" id="form">
                    <div class="form-group mb-4">
                        <input style="height: 50px; font-size: 20px" type="text" name="title" id="title" class="form-control" placeholder="Enter title for your article">
                    </div>

                    <div class="form-group">
                        <textarea style="font-size: 20px" name="description" id="description" class="form-control" rows="5" placeholder="Enter Description"></textarea>
                    </div>

                    <div class="form-group mb-3">
                        <textarea style="font-size: 20px" name="markdown" id="markdown" class="form-control" placeholder="Enter Markdown"></textarea>
                    </div>

                    <!-- image upload -->

                    <div class="form-group mb-4 text-center
                        justify-content-center">
                        <input style="height: 50px; font-size: 20px;" type='file' name="file" id="image" class="form-control" accept="image/*" multiple>
                    </div>

                    <div class="new_blog form-group">
                        <div class="btn-group my-4">
                            <input class="btn btns" name="category" list="browsers" placeholder="Select Category" id="category">
                            <datalist id="browsers">
                <option value="Physics">
                <option value="Chemistry">
                <option value="Math">
              </datalist>
                        </div>
                    </div>

                    <a href="/NormalAdminDashboard" class="btn btn-secondary btns mx-3">Cancel</a>
                    <button class="btn btn-info btns mx-3" type="submit">Save</button>
                </form>
            </div>
        </div>
    </section>





