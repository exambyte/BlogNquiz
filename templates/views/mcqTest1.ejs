  <section class="start-quiz" id="start-quiz">
    <div class="row1">
      <div class="row2">
        <p id="qtn">Question : Loading</p>
      </div>
      <div id="question-area" class="ques-area">
        <ul>
          <li>
            <input type="radio" name="answer" id="ans1" class="answer">
            <label for="ans1" id="option1"> Loading...</label>
          </li>
          <li>
            <input type="radio" name="answer" id="ans2" class="answer">
            <label for="ans2" id="option2">  Loading...</label>
          </li>
          <li>
